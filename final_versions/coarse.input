	CUMACE->OPHIUR*IsoCUMACE + ISOPOD->OPHIUR*IsoISOPOD + &
	DETAMP->OPHIUR*IsoDETAMP + DPAMP->OPHIUR*IsoDPAMP + &
	DETSHR->OPHIUR*IsoDETSHR + OPHIUR->OPHIUR*IsoOPHIUR + &
	ECHCOR->OPHIUR*IsoECHCOR + DETPOL->OPHIUR*IsoDETPOL + &
	NEPHTY->OPHIUR*IsoNEPHTY + CARPOLP->OPHIUR*IsoCARPOLP + &
	CARPOLC->OPHIUR*IsoCARPOLC + CARPOLPC->OPHIUR*IsoCARPOLPC + &
	SUSWOR->OPHIUR*IsoSUSWOR + SUSBIV->OPHIUR*IsoSUSBIV + &
	HSUDEBI->OPHIUR*IsoHSUDEBI + SUDEBI->OPHIUR*IsoSUDEBI + &
	BROGAS->OPHIUR*IsoBROGAS + SUSGAS->OPHIUR*IsoSUSGAS + &
	SOM->OPHIUR*IsoSOM + SPOM->OPHIUR*IsoSPOM

