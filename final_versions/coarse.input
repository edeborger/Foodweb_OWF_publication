	!SOM->ECHCOR

!-- Detritivore polychaetes
	!SOM->DETPOL

!-- Nephtyds
	NEPHTYmassbalance: flowto(NEPHTY)*IsoNEPHTY = flowto(NEPHTY)*FracNEPHTY + &
	DETPOL->NEPHTY*IsoDETPOL + NEPHTY->NEPHTY*IsoNEPHTY + &
	CARPOLP->NEPHTY*IsoCARPOLP + CARPOLC->NEPHTY*IsoCARPOLC + &
	CARPOLPC->NEPHTY*IsoCARPOLPC + SUSWOR->NEPHTY*IsoSUSWOR + &
	SUSBIV->NEPHTY*IsoSUSBIV + HSUDEBI->NEPHTY*IsoHSUDEBI + &
	SUDEBI->NEPHTY*IsoSUDEBI + SOM->NEPHTY*IsoSOM

!-- Carnivorous polychaetes P
	CARPOLPmassbalance: flowto(CARPOLP)*IsoCARPOLP = flowto(CARPOLP)*FracCARPOLP + &
	DETPOL->CARPOLP*IsoDETPOL + NEPHTY->CARPOLP*IsoNEPHTY + &
	CARPOLP->CARPOLP*IsoCARPOLP + CARPOLC->CARPOLP*IsoCARPOLC + &
	CARPOLPC->CARPOLP*IsoCARPOLPC + SUSWOR->CARPOLP*IsoSUSWOR + &
	SOM->CARPOLP*IsoSOM

!-- Carnivorous polychaetes C
	CARPOLCmassbalance: flowto(CARPOLC)*IsoCARPOLC = flowto(CARPOLC)*FracCARPOLC + &
	CUMACE->CARPOLC*IsoCUMACE + ISOPOD->CARPOLC*IsoISOPOD + &
	DETAMP->CARPOLC*IsoDETAMP + DPAMP->CARPOLC*IsoDPAMP + &
	DETSHR->CARPOLC*IsoDETSHR + SOM->CARPOLC*IsoSOM

!-- Carnivorous polychaetes PC
	CARPOLPCmassbalance: flowto(CARPOLPC)*IsoCARPOLPC = flowto(CARPOLPC)*FracCARPOLPC + &
	CUMACE->CARPOLPC*IsoCUMACE + ISOPOD->CARPOLPC*IsoISOPOD + &
	DETAMP->CARPOLPC*IsoDETAMP + DPAMP->CARPOLPC*IsoDPAMP + &
	DETSHR->CARPOLPC*IsoDETSHR + DETPOL->CARPOLPC*IsoDETPOL + &
	NEPHTY->CARPOLPC*IsoNEPHTY + CARPOLP->CARPOLPC*IsoCARPOLP + &
	CARPOLC->CARPOLPC*IsoCARPOLC + CARPOLPC->CARPOLPC*IsoCARPOLPC + &
	SUSWOR->CARPOLPC*IsoSUSWOR + SOM->CARPOLPC*IsoSOM

!-- Suspension feeding worms
	SUSWORmassbalance: flowto(SUSWOR)*IsoSUSWOR = flowto(SUSWOR)*FracSUSWOR + &
	ZOOPLANKT->SUSWOR*IsoZOOPLANKT + PHYTOPLANKT->SUSWOR*IsoPHYTOPLANKT + &
	SPOM->SUSWOR*IsoSPOM

!-- Suspension feeding bivalves
	SUSBIVmassbalance: flowto(SUSBIV)*IsoSUSBIV = flowto(SUSBIV)*FracSUSBIV + &
	ZOOPLANKT->SUSBIV*IsoZOOPLANKT + PHYTOPLANKT->SUSBIV*IsoPHYTOPLANKT + &
	SPOM->SUSBIV*IsoSPOM

!-- Herbivorous suspension-deposit feeding bivalves
	HSUDEBImassbalance: flowto(HSUDEBI)*IsoHSUDEBI = flowto(HSUDEBI)*FracHSUDEBI + &
	PHYTOPLANKT->HSUDEBI*IsoPHYTOPLANKT + SOM->HSUDEBI*IsoSOM + &
	SPOM->HSUDEBI*IsoSPOM

!-- Suspension deposit feeding bivalves
	SUDEBImassbalance: flowto(SUDEBI)*IsoSUDEBI = flowto(SUDEBI)*FracSUDEBI + &
	ZOOPLANKT->SUDEBI*IsoZOOPLANKT + PHYTOPLANKT->SUDEBI*IsoPHYTOPLANKT + &
	SOM->SUDEBI*IsoSOM + SPOM->SUDEBI*IsoSPOM

!-- Browsing gastropods
	!SOM->BROGAS*IsoSOM

!-- Suspension feeding gastropods
	SUSGASmassbalance: flowto(SUSGAS)*IsoSUSGAS = flowto(SUSGAS)*FracSUSGAS + &
	PHYTOPLANKT->SUSGAS*IsoPHYTOPLANKT + SPOM->SUSGAS*IsoSPOM

!-- Sepiola atlantica
	SEPATLmassbalance: flowto(SEPATL)*IsoSEPATL = flowto(SEPATL)*FracSEPATL + &
	GOBIFO->SEPATL*IsoGOBIFO + CUMACE->SEPATL*IsoCUMACE + &
	ISOPOD->SEPATL*IsoISOPOD + DETAMP->SEPATL*IsoDETAMP + &
	DPAMP->SEPATL*IsoDPAMP + DETSHR->SEPATL*IsoDETSHR + &
	SUDECR->SEPATL*IsoSUDECR + PAGURI->SEPATL*IsoPAGURI + &
	CARNCR->SEPATL*IsoCARNCR + ZOOPLANKT->SEPATL*IsoZOOPLANKT

!-- SOM burial flux
	SOMbur = SOMburial

