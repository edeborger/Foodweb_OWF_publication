!-- Asterias rubens
	ASTRUBmassbalance: flowto(ASTRUB)*IsoASTRUB = flowto(ASTRUB)*FracASTRUB + &
	CUMACE->ASTRUB*IsoCUMACE + ISOPOD->ASTRUB*IsoISOPOD + &
	DETAMP->ASTRUB*IsoDETAMP + DPAMP->ASTRUB*IsoDPAMP + &
	SUDECR->ASTRUB*IsoSUDECR + PAGURI->ASTRUB*IsoPAGURI + &
	CARNCR->ASTRUB*IsoCARNCR + ASTRUB->ASTRUB*IsoASTRUB + &
	DETPOL->ASTRUB*IsoDETPOL + NEPHTY->ASTRUB*IsoNEPHTY + &
	CARPOLP->ASTRUB*IsoCARPOLP + CARPOLC->ASTRUB*IsoCARPOLC + &
	CARPOLPC->ASTRUB*IsoCARPOLPC + SUSWOR->ASTRUB*IsoSUSWOR + &
	SUSBIV->ASTRUB*IsoSUSBIV + HSUDEBI->ASTRUB*IsoHSUDEBI + &
	SUDEBI->ASTRUB*IsoSUDEBI + BROGAS->ASTRUB*IsoBROGAS + &
	SUSGAS->ASTRUB*IsoSUSGAS + SOM->ASTRUB*IsoSOM

!-- Ophiurids
